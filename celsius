//Programa que peça a temperatura em graus Celsius, transforme e mostre em graus Farenheit.

#include <stdio.h>

int main()
{
    float C;
    float F;
    
    printf("Digite a temperatura em Celsius: ");
    scanf("%f", &C);
    
    F = (C * 9/5) + 32;
    
    printf("A temperatura em Fahrenheit é: %.2f", F );
    
    return 0;
}
