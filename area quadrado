//Faça um Programa que calcule a área de um quadrado, em seguida mostre o dobro desta área para o usuário.

#include <stdio.h>

int main()
{
    float base;
    float altura;
    float a;
    float t;
    
    printf("Digite a base de um quadrado: ");
    scanf("%f", &base);
    
    printf("Digite a altura de um quadrado: ");
    scanf("%f", &altura);
    
    a = base * altura;
    t = a * 2;
    
    printf("O dobro da área é: %.2f", t);
    
    return 0;
}
